        <TextBox x:Name="midnamebox" HorizontalAlignment="Left" Height="26" Margin="216,124,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="166"/>
        <TextBox x:Name="logbox" HorizontalAlignment="Left" Height="26" Margin="10,203,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="166"/>
        <TextBox x:Name="passbox" HorizontalAlignment="Left" Height="26" Margin="216,203,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="166"/>
        <TextBox x:Name="phonebox" HorizontalAlignment="Left" Height="26" Margin="216,282,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="166"/>
        <ComboBox x:Name="postbox" HorizontalAlignment="Left" Height="26" Margin="10,282,0,0" VerticalAlignment="Top" Width="152"/>
        <ComboBox x:Name="genbox" HorizontalAlignment="Left" Height="26" Margin="116,331,0,0" VerticalAlignment="Top" Width="46"/>

