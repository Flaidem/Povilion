        <Label Content="Пол" HorizontalAlignment="Left" Height="26" Margin="10,331,0,0" VerticalAlignment="Top" Width="67"/>
        <Button Content="Выбрать" HorizontalAlignment="Left" Height="26" Margin="273,370,0,0" VerticalAlignment="Top" Width="91" Click="Serchphoto"/>
        <Button Content="Добавить" HorizontalAlignment="Left" Height="26" Margin="216,433,0,0" VerticalAlignment="Top" Width="166" Click="addemp"/>
        <Label Content="Фото" HorizontalAlignment="Left" Height="26" Margin="10,370,0,0" VerticalAlignment="Top" Width="152"/>
        <TextBox x:Name="namebox" HorizontalAlignment="Left" Height="26" Margin="216,27,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="166"/>
