        xmlns:local="clr-namespace:Povilion.STR"
        mc:Ignorable="d"
        Icon="/STR/KingIt.png"
        Title="Добавление ТЦ" Height="500" Width="400" >
    <Grid>
        <Label Content="Имя" HorizontalAlignment="Left" Height="26" Margin="10,27,0,0" VerticalAlignment="Top" Width="152"/>
        <Label Content="Фамилия" HorizontalAlignment="Left" Height="26" Margin="10,75,0,0" VerticalAlignment="Top" Width="152"/>
        <Label Content="Отчество" HorizontalAlignment="Left" Height="26" Margin="10,124,0,0" VerticalAlignment="Top" Width="152"/>
